{
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.8.1",
    "@material-ui/icons": "^3.0.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-to-json": "^3.3.5",
    "isomorphic-unfetch": "^3.0.0",
    "koa": "^2.6.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "next": "^8.0.0",
    "next-redux-saga": "^4.0.0",
    "next-redux-wrapper": "^2.1.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-fade-in": "^0.1.6",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-cli-logger": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "styled-components": "^4.1.3",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-define": "^1.3.1",
    "codecov": "^3.1.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^5.11.1",
    "eslint-config-jsdoc-strict": "^2.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-redux": "^1.0.6",
    "eslint-plugin-jsdoc": "^18.6.1",
    "eslint-plugin-prettier": "^3.0.1",
    "globby": "^9.1.0",
    "husky": "^2.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "redux-mock-store": "^1.5.3"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --list-different '**/*'",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "test": "jest",
    "heroku-postbuild": "yarn build"
  },
  "lint-staged": {
    "*": [
      "prettier --list-different"
    ],
    "*.{js,jsx}": [
      "eslint",
      "jest --bail --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "react-redux",
      "jsdoc-strict",
      "plugin:prettier/recommended",
      "prettier",
      "prettier/react",
      "prettier/unicorn"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".jsx"
          ]
        }
      ],
      "fp/no-class": "off",
      "fp/no-let": "off",
      "fp/no-loops": "off",
      "fp/no-mutating-methods": "off",
      "fp/no-mutation": "off",
      "fp/no-rest-parameters": "off",
      "fp/no-this": "off",
      "fp/no-throw": "off",
      "jsdoc/no-undefined-types": "off",
      "jsdoc/require-example": "off"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setup-tests.js"
  }
}
